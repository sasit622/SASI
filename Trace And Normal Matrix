#include <stdio.h>
#include <math.h>

int main() {
    int m, n;
    
    // Input for the dimensions of the matrix
    scanf("%d %d", &m, &n);
    
    // Check if it's a square matrix
    if (m != n) {
        printf("Matrix must be square!\n");
        return 0;
    }

    int matrix[m][n];
    double normal = 0;
    int trace = 0;

    // Input for matrix elements
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &matrix[i][j]);
            // Calculate normal as sum of squares of all elements
            normal += matrix[i][j] * matrix[i][j];
            // Calculate trace as sum of diagonal elements
            if (i == j) {
                trace += matrix[i][j];
            }
        }
    }

    // Take the square root for the normal
    normal = sqrt(normal);

    // Output the trace and normal
    printf("%d %.2lf\n", trace, normal);

    return 0;
}
